<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.sagu.mapper.EstateMapperInter">
	<insert id="insertEstate" parameterType="estate">
		insert into real_estates(post_id,property_type,area,rooms,floor,deal_type) values
		(#{postId},#{propertyType},#{area},#{rooms},#{floor},#{dealType})
	</insert>
	<select id="getOneEstateData" resultType="estate" parameterType="Long">
		select * from real_estates where post_id=#{postId}
	</select>
</mapper>