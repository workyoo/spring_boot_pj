<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.sagu.mapper.ItemMapperInter">
   <insert id="insertItem" parameterType="item">
      insert into used_items(post_id,category_id,conditions) values
      (#{postId},#{categoryId},#{conditions})
   </insert>
   <select id="getOneItemData" resultType="item" parameterType="Long">
      select * from used_items where post_id=#{postId}
   </select>
</mapper>